{
	"name": "markdownpreview",
	"displayName": "MarkdownPreview",
	"description": "Replace all variables and fragements, then you can preview complete markdown",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:markdownPreview.start"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "markdownPreview.start",
				"title": "Zilliz",
				"category": "Markdown Preview"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == markdown",
					"command": "markdownPreview.start",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"markdown-it": "^11.0.0",
		"markdown-it-container": "^2.0.0",
		"markdown-it-emoji": "^1.4.0"
	}
}