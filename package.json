{
	"name": "markdown-fragment-variable-preview",
	"displayName": "MarkdownPreview",
	"description": "Support variables and fragements, when you save md file will replace it, then you can preview complete markdown",
	"version": "0.1.5",
	"author": "chenzizhao",
	"publisher": "chenzizhao",
	"repository": {
		"type": "git",
		"url": " https://github.com/nameczz/markdown-extension"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:markdownPreview.start",
		"onCommand:markdownPreview.light"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "markdownPreview.start",
				"title": "Zilliz",
				"category": "Markdown Preview"
			},
			{
				"command": "markdownPreview.light",
				"title": "Light Theme",
				"category": "Markdown Preview Light"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == markdown",
					"command": "markdownPreview.start",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"start": "webpack --mode development --watch"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"css-loader": "^3.5.3",
		"markdown-it": "^11.0.0",
		"markdown-it-container": "^2.0.0",
		"markdown-it-emoji": "^1.4.0",
		"md2md": "^0.5.1",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.2.1",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	}
}